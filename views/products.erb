<ul>
  <% @products.each do |product| %>
    <li> <a href="products/<%=product["id"]%>"> View <%= product["name"]%></a></li>
  <% end %>
</ul>

<ul>
	<% @categories.each do |category| %>
	<li> [<%=category['id']%>]  <%=category['name']%> </li>
	<% end %>
</ul>

<a href="products/new"> Create a new product.</a>
